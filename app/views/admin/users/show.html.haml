%h1 Showing user

%table.show-table
  %tr
    %td ID:
    %td= @user.id
  %tr
    %td First Name:
    %td= @user.first_name
  %tr
    %td Last Name:
    %td= @user.last_name
  %tr
    %td Display Name:
    %td= @user.display_name
  %tr
    %td About Me:
    %td= @user.about_me
  %tr
    %td Email:
    %td= @user.email
  %tr
    %td Reset Password Sent at:
    %td= @user.reset_password_sent_at
  %tr
    %td Sign-in Count:
    %td= @user.sign_in_count
  %tr
    %td Current Sign-in at:
    %td= @user.current_sign_in_at
  %tr
    %td Current Sign-in IP:
    %td= @user.current_sign_in_ip
  %tr
    %td Last Sign-in at:
    %td= @user.last_sign_in_at
  %tr
    %td Last Sign-in IP:
    %td= @user.last_sign_in_ip
  %tr
    %td Failed Attempts:
    %td= @user.failed_attempts
  %tr
    %td Locked at:
    %td= @user.locked_at
  %tr
    %td Created at:
    %td= @user.created_at
  %tr
    %td Updated at:
    %td= @user.updated_at

= link_to 'Edit', edit_admin_user_path(@user)
\|
= link_to 'Back', admin_users_path
